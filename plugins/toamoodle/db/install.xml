<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/etl/plugins/toamoodle/db" VERSION="20090921" COMMENT="XMLDB file for Moodle local/etl/plugins/toamoodle"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="localetl_toamoodle" COMMENT="Default comment for toa for moodle, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
        <FIELD NAME="wwwroot" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Relative to wwwroot"/>
        <FIELD NAME="publickey" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="lastextract" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="lastextractactions" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="lastextractacademics" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="lastextractdocuments" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="lastextractcommunications" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="lastextractgrades" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="outputencoding" TYPE="char" LENGTH="16" NOTNULL="true" DEFAULT="ISO-8859-1" SEQUENCE="false"/>
        <FIELD NAME="accessurl" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Url to the external Web TAO service"/>
        <FIELD NAME="externalaccessurl" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Url to a non logged access door to TAO"/>
        <FIELD NAME="toahost" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Host for Refer based check"/>
        <FIELD NAME="toaipmask" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Mask for IP based check"/>
        <FIELD NAME="masquerade" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0" COMMENT="Mask for IP based check"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>